<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IrregularLayout">
        <attr name="ItemHorSpace" format="dimension"/>
        <attr name="ItemVerSpace" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="PageTabIndicator">
        <attr name="IndicatorTextColor" format="color"/>
        <attr name="IndicatorTextSelectColor" format="color"/>
        <attr name="IndicatorTextSize" format="dimension"/>
        <attr name="IndicatorLineColor" format="color"/>
        <attr name="IndicatorLineHeight" format="dimension"/>
        <attr name="IndicatorBottomLineColor" format="color"/>
        <attr name="IndicatorBottomLineHeight" format="dimension"/>
        <attr name="IndicatorBottomLineShow" format="boolean"/>
        <attr name="IndicatorAverageScreen" format="boolean"/>
        <attr name="IndicatorFillWidthWithTab" format="boolean"/>
        <attr name="IndicatorLeftRightPadding" format="dimension"/>
        <attr name="IndicatorTwoTabDistance" format="dimension"/>
        <attr name="IndicatorDrawTabBackground" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="WaveLoadingView">
        <attr name="WaveBaseLine" format="dimension"/>
        <attr name="WaveHeight" format="dimension"/>
        <attr name="WaveColor" format="color"/>
        <attr name="WaveFastColor" format="color"/>
        <attr name="WaveCircleThickness" format="dimension"/>
        <attr name="WaveCircleBackground" format="color"/>
        <attr name="WaveUpAndDown" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="WaveCircleLoadingView">
        <attr name="WaveBaseLineHeader" format="dimension"/>
        <attr name="WaveHeightHeader" format="dimension"/>
        <attr name="WaveColorHeader" format="color"/>
        <attr name="WaveFastColorHeader" format="color"/>
        <attr name="WaveCircleThicknessHeader" format="dimension"/>
        <attr name="WaveCircleBackgroundHeader" format="color"/>
        <attr name="WaveUpAndDownHeader" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CircleProgressView">
        <attr name="LightColor" format="color"/>
        <attr name="DarkColor" format="color"/>
        <attr name="CircleRadius" format="dimension"/>
        <attr name="ScaleValue" format="integer"/>
        <attr name="Thickness" format="dimension"/>
        <attr name="BallRadius" format="dimension"/>
    </declare-styleable>
</resources>