<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xulc.algorithmstudy.ui.RoundImageActivity">
    <com.xulc.algorithmstudy.widget.RoundImageView
        android:id="@+id/roundIv"
        android:layout_width="200dp"
        android:layout_height="400dp"
        android:src="@mipmap/zlyh"
        app:RoundType="corner"
        app:CornerY="20dp"
        app:CornerX="20dp"
        android:background="@color/red" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="换图"
        android:layout_gravity="bottom"
        android:onClick="changePicture"/>
    <com.xulc.algorithmstudy.widget.TestView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="right"/>
</FrameLayout>
